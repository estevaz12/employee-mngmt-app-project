<% table_heads = %w(Name Phone Current\ Assignment Role Active) %>

<table class="striped highlight">
  <thead>
    <% for th in table_heads %>
      <th><%= th %></th>
    <% end %>
  </thead>
  <% for i in list %>
    <%= render partial: "search_table_data",
               locals: {obj: i, list: list} %>
  <% end %>
</table>
